{
  "name": "discover-blockchain-frontend",
  "description": "Discover Blockchain",
  "website": "https://discoverblockchain.io/",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "ng serve",
    "build": "ng build --prod",
    "deploy": "ng build --prod --base-href / && angular-cli-ghpages",
    "test": "ng test --watch=false --code-coverage",
    "e2e": "ng e2e --prod",
    "lint": "ng lint",
    "ci": "npm run lint && npm run test && npm run e2e",
    "sme": "ng build && source-map-explorer dist/main.bundle.js",
    "release": "standard-version && git push --follow-tags origin master",
    "docker": "docker build -t discoverblockchain . && docker run -d -p 4200:80 discoverblockchain"
  },
  "private": true,
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "dependencies": {
    "@angular/animations": "^5.2.3",
    "@angular/common": "^5.2.3",
    "@angular/compiler": "^5.2.3",
    "@angular/core": "^5.2.3",
    "@angular/flex-layout": "5.0.0-beta.14",
    "@angular/forms": "^5.2.3",
    "@angular/http": "^5.2.3",
    "@angular/platform-browser": "^5.2.3",
    "@angular/platform-browser-dynamic": "^5.2.3",
    "@angular/router": "^5.2.3",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.1",
    "angular2-highcharts": "^0.5.5",
    "bignumber.js": "^5.0.0",
    "bootstrap": "^4.0.0",
    "classlist.js": "1.1.20150312",
    "core-js": "^2.5.3",
    "crypto-socket": "^2.0.3",
    "font-awesome": "^4.7.0",
    "jquery": "^1.9.1",
    "modernizr": "^3.5.0",
    "popper.js": "^1.12.9",
    "rxjs": "^5.5.6",
    "solc": "^0.4.19",
    "truffle-contract": "^3.0.4",
    "web3": "github:ethereum/web3.js",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cdk": "^5.1.1",
    "@angular/cli": "~1.7.4",
    "angular-cli-ghpages": "0.5.2",
    "@angular/compiler-cli": "^5.2.3",
    "@angular/material": "^5.1.1",
    "@angular/language-service": "^5.2.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/mocha": "^2.2.45",
    "codelyzer": "^4.1.0",
    "coveralls": "3.0.0",
    "hammerjs": "2.0.8",
    "jasmine-core": "~2.9.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-istanbul": "0.6.0",
    "protractor": "~5.2.0",
    "source-map-explorer": "1.5.0",
    "standard-version": "4.3.0",
    "ts-helpers": "1.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.6.2",
    "web3-typescript-typings": "^0.9.0"
  }
}
